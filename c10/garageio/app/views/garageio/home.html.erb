<h1>Sweet Rides</h1>
	<% if @cars.length != 0 %>
		<table>
			<% @cars.each do |item| %>
			<tr>
			<td><%=item.Make%> </td>
			<td> <%= item.Model %></td>
			<td> <%= item.Year %> </td>
			<td> <%= item.Color %> </td>
			<td><%=item.Nickname %></td>
			</tr>
			<% end %>
		</table>

	<% else %>
		<h2> No Cars Here ... </h2>
	<% end %>